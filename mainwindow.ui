     <property name="styleSheet">
      <string notr="true">/* Style de base du QLabel */
QLabel {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #e0f7fa, stop:1 #ffffff); /* Dégradé doux du bleu clair au blanc */
    color: #333; /* Couleur du texte */
    font-weight: bold;
    font-size: 15px;
    padding: 12px 18px; /* Espacement interne pour un aspect spacieux */
    border-radius: 12px; /* Coins légèrement arrondis */
    margin-bottom: 12px; /* Espacement en bas */
    border: 1px solid #b2ebf2; /* Bordure fine assortie */
    box-shadow: 0px 6px 15px rgba(0, 0, 0, 0.1); /* Ombre subtile */
    transition: all 0.3s ease; /* Transition douce pour les effets de survol */
}

/* Variante pour un QLabel de type alerte */
QLabel.alert {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #ffcccb, stop:1 #ffffff); /* Dégradé rouge pâle */
    color: #d32f2f;
    border: 1px solid #ef9a9a;
    box-shadow: 0px 6px 15px rgba(211, 47, 47, 0.2); /* Ombre rougeâtre plus prononcée */
}

/* Variante pour un QLabel de type succès */
QLabel.success {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #c8e6c9, stop:1 #ffffff); /* Dégradé vert clair */
    color: #388e3c;
    border: 1px solid #a5d6a7;
    box-shadow: 0px 6px 15px rgba(56, 142, 60, 0.2); /* Ombre verte plus prononcée */
}

/* Effet de survol pour tous les QLabel */
QLabel:hover {
    box-shadow: 0px 8px 20px rgba(0, 0, 0, 0.15); /* Augmente l'ombre pour un effet interactif */
    transform: scale(1.02); /* Légère augmentation de la taille au survol */
}
</string>
